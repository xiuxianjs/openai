    "@types/ws": "8.18.1",
    "alemonjs": "^2.1.0-alpha.21",
    "cssnano": "^7.0.6",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
